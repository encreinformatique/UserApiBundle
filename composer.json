{
  "name": "encreinformatique/user-api-bundle",
  "license": "MIT",
  "type": "symfony-bundle",
  "autoload": {
    "psr-4": {
      "EncreInformatique\\UserApiBundle\\": ""
    },
    "exclude-from-classmap": [
      "Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "Tests/"
    }
  },
  "require": {
    "php": ">=7.1",
    "friendsofsymfony/rest-bundle": "^2.5|^3.0",
    "lexik/jwt-authentication-bundle": "^2.6",
    "nelmio/api-doc-bundle": "^3.0",
    "sensio/framework-extra-bundle": "^5.0",
    "symfony/config": "^3.3|^4.0|^5.0",
    "symfony/http-kernel": "^3.3|^4.0|^5.0",
    "symfony/security-bundle": "^3.3|^4.0|^5.0"
  },
  "require-dev": {
    "doctrine/doctrine-bundle": "^1.3",
    "doctrine/orm": "^2.5.11",
    "phpunit/phpunit": "^6,5|^7.1"
  }
}
